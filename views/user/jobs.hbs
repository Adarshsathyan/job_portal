<section class="section section-bg" id="call-to-action"
        style="background-image: url(user/images/banner-image-1-1920x500.jpg)">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 offset-lg-1">
                    <div class="cta-content">
                        <br>
                        <br>
                        <h2>Our <em>Jobs</em></h2>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ***** Call to Action End ***** -->

    <!-- ***** Fleet Starts ***** -->
    <section class="section" id="trainers">
        <div class="container">
            <br>
            <br>

            <div class="row">
                

                <div class="col-md-12">
                    <div class="row">
                        {{#each jobs}}
                        <div class="col-md-4">
                            <div class="trainer-item">
                                <div class="image-thumb">
                                    <img src="/employee/job_images/{{this._id}}.jpg" style="width: 322px; height:241px;" alt="">
                                </div>
                                <div class="down-content">
                                    <span> <sup>$</sup>{{this.salary}}</span>

                                    <h4>{{this.designation}}</h4>

                                    <!--<p>Medical &nbsp;/&nbsp; Health Jobs</p>-->

                                    <ul class="social-icons">
                                        <li><a href="/jobdetails/{{this._id}}">+ View More</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <br>

            

        </div>
    </section>
    <!-- ***** Fleet Ends ***** -->


    <!-- ***** Footer Start ***** -->
    