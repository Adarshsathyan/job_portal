<div class="">
	<div id="page-wrapper">
		<div class="header">
			<h1 class="page-header">
				Dashboard
			</h1>
			<ol class="breadcrumb">
				<li><a href="#">Home</a></li>
				<li><a href="#">Dashboard</a></li>
				<li class="active">Data</li>
			</ol>
 
		</div>
		<div id="page-inner">

			<div class="dashboard-cards">
				<div class="row">
					<div class="col-xs-12 col-sm-6 col-md-3">

						<div class="card horizontal cardIcon waves-effect waves-dark"style="height:153px;">
							<div class="card-image red">
								<i class="material-icons dp48">import_export</i>
							</div>
							<div class="card-stacked red">
								<div class="card-content">
									<h3 id="jobs">{{jobs}}</h3>
								</div>
								<div class="card-action">
									<strong>JOBS</strong>
								</div>
							</div>
						</div>

					</div>
					<div class="col-xs-12 col-sm-6 col-md-3">

						<div class="card horizontal cardIcon waves-effect waves-dark" style="height:153px;">
							<div class="card-image orange">
								<i class="material-icons dp48">shopping_cart</i>
							</div>
							<div class="card-stacked orange">
								<div class="card-content">
									<h3 id="employees">{{employees}}</h3>
								</div>
								<div class="card-action">
									<strong>EMPLOYEES</strong>
								</div>
							</div>
						</div>
					</div>
					<div class="col-xs-12 col-sm-6 col-md-3">

						<div class="card horizontal cardIcon waves-effect waves-dark" style="height:153px;">
							<div class="card-image blue">
								<i class="material-icons dp48">equalizer</i>
							</div>
							<div class="card-stacked blue">
								<div class="card-content">
									<h3 id="users">{{users}}</h3>
								</div>
								<div class="card-action">
									<strong>USERS</strong>
									
								</div>
							</div>
						</div>

					</div>
					<div class="col-xs-12 col-sm-6 col-md-3">

						<div class="card horizontal cardIcon waves-effect waves-dark" style="height:153px;">
							<div class="card-image green">
								<i class="material-icons dp48">supervisor_account</i>
							</div>
							<div class="card-stacked green">
								<div class="card-content">
									<h3 id="revenue">{{revenue}}</h3>
								</div>
								<div class="card-action">
									<strong>Revenue</strong>
								</div>
							</div>
						</div>
						<div style="display: none;">
						<p id="applicaton">{{application}}</p>
						<p id="approved">{{approved}}</p>
						<p id="rejected">{{rejected}}</p>
						<p id="category">{{category}}</p>
						</div>
						
					</div>
				</div>
			</div>
			
			<div class="row">
				<div class="col-md-6">
					<h4 style="padding: 3px;">Jobs Status</h4>
				<canvas  id="empChart"></canvas>
				</div>
				<div class="col-md-6">
					<h4 style="padding: 3px;">Overall Status</h4>
					<canvas  id="adminChart"></canvas>
				
				<script src="/admin/javascripts/admingraph.js"></script>
			</div>
			</div>
			
		</div>
		
	</div>
	
</div>


